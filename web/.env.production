# Production Environment for kinforge.xyz

# WalletConnect Project ID - Get one at https://cloud.walletconnect.com
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=YOUR_WALLETCONNECT_PROJECT_ID

# Chain ID: 56 for BSC Mainnet, 97 for BSC Testnet
NEXT_PUBLIC_CHAIN_ID=56

# API URL - Railway backend
NEXT_PUBLIC_API_URL=https://houseforgeserver-production.up.railway.app

# Contract Addresses - BSC Mainnet (V3 - new economics)
NEXT_PUBLIC_AGENT_CONTRACT=0x713Be3D43c5DdfE145215Cd366c553c75A06Ce7f
NEXT_PUBLIC_FUSION_CONTRACT=0xa62E109Db724308FEB530A0b00431cf47BBC1f6E

# BSC Mainnet RPC
NEXT_PUBLIC_RPC_URL=https://bsc-dataseed.binance.org

# Analytics
NEXT_PUBLIC_ANALYTICS_ENABLED=true
