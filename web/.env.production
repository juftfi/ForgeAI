NEXT_PUBLIC_FUSION_CONTRACT=0x8a7fdf8e6b3E7C23744de8eE893D0C1899189004

# BSC Mainnet RPC
NEXT_PUBLIC_RPC_URL=https://bsc-dataseed.binance.org

# Analytics
NEXT_PUBLIC_ANALYTICS_ENABLED=true
