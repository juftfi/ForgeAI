# WalletConnect Project ID - Get one at https://cloud.walletconnect.com
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_project_id_here

# Chain ID: 56 for BSC Mainnet, 97 for BSC Testnet
NEXT_PUBLIC_CHAIN_ID=97

# API URL for backend services
NEXT_PUBLIC_API_URL=http://localhost:3001

# Contract Addresses (update after deployment)
NEXT_PUBLIC_AGENT_CONTRACT=0x0000000000000000000000000000000000000000
NEXT_PUBLIC_FUSION_CONTRACT=0x0000000000000000000000000000000000000000

# RPC URL (optional, uses public RPC if not set)
NEXT_PUBLIC_RPC_URL=

# Analytics (optional)
NEXT_PUBLIC_ANALYTICS_ENABLED=false
